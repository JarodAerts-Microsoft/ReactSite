{ 
  "routes": [ 
    {
      "route": "/login",
      "serve": "/.auth/login/github"
    },
    {
       "route" : "/shoppingList/*",
       "serve" : "/index.html",
       "statusCode" : 200
    },
    {
      "route": "/logout",
      "serve": "/.auth/logout"
    },
    {
      "route": "/foo1",
      "statusCode": "200",
      "serve": "/about"
    },
    {
      "route": "/bar",
      "statusCode": "301",
      "allowedRoles": ["admin"],
      "serve": "/about"
    },
    {
      "route": "/baz",
      "statusCode": "302",
      "serve": "/about"
    },
    {
      "route": "/about/*",
      "statusCode": "200",
      "allowedRoles": ["authenticated", "admin"],
      "serve": "/about"
    },
    {
      "route": "/admin",
      "statusCode": "200",
      "serve": "/admin"
    },
    {
      "route": "/api/*",
      "allowedRoles": ["anonymous", "authenticated", "admin"]
    },
    {
      "route": "/.auth/complete",
      "serve": "/about",
      "statusCode": 429,
    },
    {
      "route": "/me",
      "serve": "/.auth/me"
    },
    {
      "route": "/foo bar",
      "serve": "/.auth/login/facebook"
    },
    {
      "route": "foo2020",
      "serve": "/foo1 bar1"
    },
    {
      "route": "bar2020",
      "serve": "/foo+bar",
      "statusCode": 302
    },
    {
      "route": "/news",
      "serve": "/news",
      "statusCode": 200,
      "allowedRoles": ["anonymous"]
    },
    {
        "route": "/",
        "allowedRoles": ["super_admin"]
    }
  ],
  "mimeTypes": { ".jpg": "application/json" },
  "globalHeaders": {
  "a": "b" },
  "platformErrorOverrides": {
        "NotFound": {
          "serve": "/news.html"
        },
     "Unauthenticated": {
          "statusCode": "302",
          "serve": "/.auth/login/google"
        }
    }
} 
