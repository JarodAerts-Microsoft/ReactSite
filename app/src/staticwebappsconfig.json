{ 
  "routes": [ 
    {
      "route": "/logintw",
      "rewrite": "/.auth/login/twitter"
    },
    {
      "route": "/logout",
      "rewrite": "/.auth/logout"
    },
    {
      "route": "/foo1",
      "redirect": "/api/GetList"
    },
    {
      "route": "/bar",
      "statusCode": "301",
      "allowedRoles": ["admin"],
      "redirect": "/about"
    },
    {
      "route": "/baz",
      "redirect": "/about"
    },
    {
      "route": "/about/*",
      "allowedRoles": ["authenticated", "admin"],
      "rewrite": "/about"
    },
    {
      "route": "/admin",
      "rewrite": "/admin"
    },
    {
      "route": "/me",
      "redirect": "/.auth/me"
    },
    {
      "route": "/api/PostList",
      "methods": ["PUT", "POST"],
      "allowedRoles": ["authenticated"]
    }
  ],
  "mimeTypes": { ".jpg": "application/json" },
  "globalHeaders": {
  "a": "b" },
  "responseOverrides": {
        "404": {
          "statusCode": "200",
          "rewrite": "/api/GetList"
        }
    },
  "auth": {
    "identityProviders": {
      "facebook": {
        "registration": {
          "appId": "160184675274623",
          "appSecretSettingName": "FB_SECRET_Special"
        }
      }
    }
  }
} 
