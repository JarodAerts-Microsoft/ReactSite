{ 
  "routes": [ 
    {
      "match-route": "/login",
      "serve": "/.auth/login/github"
    },
    {
       "match-route" : "/shoppingList/*",
       "serve" : "/index.html",
       "statusCode" : 200
    },
    {
      "match-route": "/logout",
      "serve": "/.auth/logout"
    },
    {
      "match-route": "/foo1",
      "statusCode": "200",
      "serve": "/about"
    },
    {
      "match-route": "/bar",
      "statusCode": "301",
      "allowedRoles": ["admin"],
      "serve": "/about"
    },
    {
      "match-route": "/baz",
      "statusCode": "302",
      "serve": "/about"
    },
    {
      "match-route": "/about/*",
      "statusCode": "200",
      "allowedRoles": ["authenticated", "admin"],
      "serve": "/about"
    },
    {
      "match-route": "/admin",
      "statusCode": "200",
      "serve": "/admin"
    },
    {
      "match-route": "/api/*",
      "allowedRoles": ["anonymous", "authenticated", "admin"]
    },
    {
      "match-route": "/.auth/complete",
      "serve": "/about",
      "statusCode": 429,
    },
    {
      "match-route": "/me",
      "serve": "/.auth/me"
    },
    {
      "match-route": "/foo bar",
      "serve": "/.auth/login/facebook"
    },
    {
      "match-route": "foo2020",
      "serve": "/foo1 bar1"
    },
    {
      "match-route": "bar2020",
      "serve": "/foo+bar",
      "statusCode": 302
    },
    {
      "match-route": "/news",
      "serve": "/news",
      "statusCode": 200,
      "allowedRoles": ["anonymous"]
    },
    {
        "match-route": "/*",
      "serve": "/index.html"
    }
  ],
  "mimeTypes": { ".jpg": "application/json" },
  "globalHeaders": {
  "a": "b" },
  "platformErrorOverrides": {
        "404": {
          "serve": "/news.html"
        },
     "401": {
          "statusCode": "302",
          "serve": "/.auth/login/google"
        }
    },
  "auth": {
    "identityProviders": {
      "facebook": {
        "registration": {
          "appId": "684017655414345",
          "appSecretSettingName": "FB_SECRET_Special"
        }
      }
    }
  }
} 
