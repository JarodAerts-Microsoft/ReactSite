{ 
  "routes": [ 
    {
      "route": "/login",
      "serve": "/.auth/login/github"
    },
    {
      "route": "/logout",
      "serve": "/.auth/logout"
    },
    {
      "route": "/.auth/login/aad",
      "statusCode": "404"
    },
    {
      "route": "/.auth/login/twitter",
      "statusCode": "404"
    },
    {
      "route": "/foo",
      "statusCode": "200",
      "serve": "/about"
    },
    {
      "route": "/bar",
      "statusCode": "301",
      "serve": "/about"
    },
    {
      "route": "/baz",
      "statusCode": "302",
      "serve": "/about"
    },
    {
      "route": "/about/*",
      "statusCode": "200",
      "serve": "/about"
    },
    {
      "route": "/admin",
      "statusCode": "200",
      "serve": "/admin"
    },
    {
      "route": "/api/*",
      "allowedRoles": ["authenticated", "admin"]
    },
    {
      "route": "/*", 
      "serve": "/index.html", 
      "statusCode": 200 
    }
  ] 
} 
