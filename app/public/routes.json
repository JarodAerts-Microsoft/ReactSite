{ 
  "routes": [ 
    {
      "route": "/login",
      "serve": "/.auth/login/github"
    },
    {
       "route" : "/shoppingList/*",
       "serve" : "/index.html",
       "statusCode" : 200
    },
    {
      "route": "/logout",
      "serve": "/.auth/logout"
    },
    {
      "route": "/foo1",
      "statusCode": "200",
      "serve": "/about"
    },
    {
      "route": "/bar",
      "statusCode": "301",
      "allowedRoles": ["admin"],
      "serve": "/about"
    },
    {
      "route": "/baz",
      "statusCode": "302",
      "serve": "/about"
    },
    {
      "route": "/about/*",
      "statusCode": "200",
      "allowedRoles": ["authenticated", "admin"],
      "serve": "/about"
    },
    {
      "route": "/admin",
      "statusCode": "200",
      "serve": "/admin"
    },
    {
      "route": "/api/*",
      "allowedRoles": ["anonymous", "authenticated", "admin"]
    },
    {
      "route": "/.auth/complete",
      "serve": "/about",
      "statusCode": 429,
    },
    {
      "route": "/me",
      "serve": "/.auth/me"
    },
  ],
  "mimeTypes":
  {
    ".jpg": "application/json",
    "png": "application/jpg"
  },
  "defaultHeaders":
  {
    "Cache-Control": "",
    "Etag": "7437589403298456543",
    "Foo": "bar",
    "X-Frame-Options": null
  }
} 
